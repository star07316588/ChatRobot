    function CheckSubjectNo() {
        var tbl2 = document.getElementById('tbl2');
        var chk = document.getElementsByName('chk');
        var txt5 = document.getElementsByName('txt5');
        var txt3 = document.getElementsByName('txt3');
        var txt9 = document.getElementsByName('txt9');
        for (i = tbl2.rows.length - 1; i > 0; i--) {
            if (chk.item(i - 1).checked == true) {
                if (!txt3.item(i - 1).value == "null" || !txt3.item(i - 1).value == "") {
                    txt5.item(i - 1).readOnly = true;
                    txt5.item(i - 1).className = "readOnlyText";
                    txt5.item(i - 1).value = "";
                    txt9.item(i - 1).readOnly = false;
                    txt9.item(i - 1).className = "inputText";
                }
                else {
                    txt5.item(i - 1).readOnly = false;
                    txt5.item(i - 1).className = "inputText";
                    txt9.item(i - 1).readOnly = true;
                    txt9.item(i - 1).className = "readOnlyText";
                }

            }
        }
    }

