                if (txt5.item(i - 1).value == "null" && txt3.item(i - 1).value != "")
                    txt5.item(i - 1).value = "";
                if (txt3.item(i - 1).value == "null" && txt5.item(i - 1).value != "")
                    txt3.item(i - 1).value = "";
            }

        }
        return flag;
    }
