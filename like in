FROM tbl_ipn_master 
WHERE 
    IPN LIKE '%6695Q%' OR
    IPN LIKE '%6704P%' OR
    IPN LIKE '%6711P%' OR
    IPN LIKE '%6714F%' OR
    IPN LIKE '%6715F%' OR
    IPN LIKE '%6725F%' OR
    IPN LIKE '%6732O%' OR
    IPN LIKE '%6733E%' OR
    IPN LIKE '%6733L%' OR
    IPN LIKE '%6735L%' OR
    IPN LIKE '%6737N%' OR
    IPN LIKE '%6737Q%' OR
    IPN LIKE '%6741M%' OR
    IPN LIKE '%6741N%' OR
    IPN LIKE '%6768F%' OR
    IPN LIKE '%6769A%'